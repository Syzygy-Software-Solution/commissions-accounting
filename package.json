{
	"name": "commissions_accounting",
	"version": "1.0.0",
	"description": "A simple CAP project.",
	"dependencies": {
		"@cap-js/hana": "^2",
		"@sap/cds": "^9",
		"@sap/xssec": "^4",
		"@sap/xsenv": "^5",
		"@sap-cloud-sdk/connectivity": "^3",
		"express": "^4",
		"nodemailer": "^6"
	},
	"devDependencies": {
		"@cap-js/sqlite": "^2",
		"@sap/cds-dk": "^9",
		"cds-plugin-ui5": "^0.13.0",
		"rimraf": "^5.0.5",
		"mbt": "^1.2.29",
		"@sap/dev-cap-tools": "^1.31.0"
	},
	"scripts": {
		"start": "cds-serve",
		"watch-commissions_accounting": "cds watch --open commissionsaccounting/index.html?sap-ui-xx-viewCache=false --livereload false",
		"undeploy": "cf undeploy commissions_accounting --delete-services --delete-service-keys --delete-service-brokers",
		"build": "rimraf resources mta_archives && mbt build --mtar archive",
		"deploy": "cf deploy mta_archives/archive.mtar --retries 1"
	},
	"private": true,
	"cds": {
		"requires": {
			"[production]": {
				"auth": "xsuaa"
			}
		}
	},
	"workspaces": [
		"app/*"
	]
}